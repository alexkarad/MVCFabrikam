@Model MVCFabrikam.Models.BlogObj

@{
    ViewBag.Title = "DisplayBlogs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DisplayBlogs</h2>

<table class="table">
    <tr>
        <th>
            Post ID
        </th>
    </tr>

@foreach (var item in @Model.Posts) 
{
    <tr>
        <td>
            @item.Post
        </td>
    </tr>
}

</table>
